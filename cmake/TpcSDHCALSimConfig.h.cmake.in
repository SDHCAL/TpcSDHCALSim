#ifndef TpcSDHCALSimConfig_h
#define TpcSDHCALSimConfig_h 1

// version macros
#define TpcSDHCALSim_MAJOR_VERSION @TpcSDHCALSim_VERSION_MAJOR@
#define TpcSDHCALSim_MINOR_VERSION @TpcSDHCALSim_VERSION_MINOR@
#define TpcSDHCALSim_PATCH_LEVEL @TpcSDHCALSim_VERSION_PATCH@
#define TpcSDHCALSim_VERSION "@TpcSDHCALSim_VERSION_MAJOR@.@TpcSDHCALSim_VERSION_MINOR@.@TpcSDHCALSim_VERSION_PATCH@"

#define TpcSDHCALSim_VERSION_GE( MAJV , MINV , PLEV )  ( (TpcSDHCALSim_MAJOR_VERSION > MAJV) || ( (TpcSDHCALSim_MAJOR_VERSION == MAJV) && (TpcSDHCALSim_MINOR_VERSION > MINV) ) || ( (TpcSDHCALSim_MAJOR_VERSION == MAJV) && (TpcSDHCALSim_MINOR_VERSION == MINV) && (TpcSDHCALSim_PATCH_LEVEL >= PLEV) ) )

#endif // TpcSDHCALSimConfig_h
